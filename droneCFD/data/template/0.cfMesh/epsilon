FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1.8239;

boundaryField
{

    xMin
    {
        type fixedValue;
        value $internalField;
    }

    xMax
    {
        type fixedValue;
        value $internalField;
    }

    yMin
    {
        type            epsilonWallFunction;
	    value $internalField;
    }
    yMax
    {
        type            epsilonWallFunction;
	    value $internalField;
    }
    zMin
    {
        type            epsilonWallFunction;
	    value $internalField;
    }
    zMax
    {
        type            epsilonWallFunction;
	    value $internalField;
    }


    patch0
    {
        type            epsilonWallFunction;
	    value $internalField;
    }
}
