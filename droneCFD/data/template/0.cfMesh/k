FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.05;

boundaryField
{
    patch0
    {
        type            slip;
    }
    xMin
    {
        type            fixedValue;
        value           uniform 0.05;
    }
    xMax
    {
        type            inletOutlet;
        inletValue      uniform 0.05;
        value           uniform 0.05;
    }
    yMin
    {
        type            slip;
    }
    yMax
    {
        type            slip;
    }
    zMin
    {
        type            slip;
    }
    zMax
    {
        type            slip;
    }
}


