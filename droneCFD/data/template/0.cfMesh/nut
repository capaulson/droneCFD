    {
        type            calculated;
        value           uniform 0;
    }
    xMin
    {
        type            calculated;
        value           uniform 0;
    }
    xMax
    {
        type            calculated;
        value           uniform 0;
    }
    yMin
    {
        type            calculated;
        value           uniform 0;
    }
    yMax
    {
        type            calculated;
        value           uniform 0;
    }
    zMin
    {
        type            calculated;
        value           uniform 0;
    }
    zMax
    {
        type            calculated;
        value           uniform 0;
    }
}


