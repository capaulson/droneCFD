FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.78;

boundaryField
{
    patch0
    {
        type            zeroGradient;
    }
    xMin
    {
        type            fixedValue;
        value           uniform 1.78;
    }
    xMax
    {
        type            inletOutlet;
        inletValue      uniform 1.78;
        value           uniform 1.78;
    }
    yMin
    {
        type            zeroGradient;
    }
    yMax
    {
        type            zeroGradient;
    }
    zMin
    {
        type            zeroGradient;
    }
    zMax
    {
        type            zeroGradient;
    }
}


